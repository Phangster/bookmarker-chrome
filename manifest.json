{
  "name": "Angkiki Bookmarker",
  "version": "1.0",
  "description": "Helps you save all URLs on the current page you are on",
  "permissions": [
    "http://angkiki-bookmarker.herokuapp.com/",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
